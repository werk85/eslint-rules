{
  "name": "eslint-config-werk85",
  "version": "7.3.0",
  "main": "index.js",
  "repository": "git@github.com:werk85/eslint-config-werk85.git",
  "author": "Malte Legenhausen <malte@werk85.de>, Andreas Krut <andreas@werk85.de>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && tsc -p tsconfig.cjs.json && tsx ./scripts/build.ts",
    "lint": "concurrently \"yarn:lint:*\"",
    "lint:es": "eslint .",
    "lint:tsc": "tsc -p tsconfig.json",
    "fix": "yarn lint:es --fix",
    "postinstall": "husky",
    "prepublishOnly": "tsx ./scripts/pre-publish.ts",
    "release": "yarn build && tsx ./scripts/release.ts"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-fp-ts": "^0.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.7",
    "@types/node": "^22.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "husky": "^9.1.4",
    "npm-check-updates": "^17.0.6",
    "rimraf": "^6.0.1",
    "tsx": "^4.17.0"
  },
  "packageManager": "yarn@3.5.0"
}
